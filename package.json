  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "npm@8.5.5",
  "dependencies": {
    "@livekit/react-core": "^1.0.3",
    "@radix-ui/react-scroll-area": "^1.0.1",
    "@stitches/react": "^1.2.8",
    "date-fns": "^2.29.3",
